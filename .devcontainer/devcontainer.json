{
    "name": "Ubuntu",
    "build": {
      "dockerfile": "Dockerfile"
    },
    "hostRequirements": {
      "cpus": 2
    },
    "customizations": {
      "codespaces": {
        "openFiles": [
            "main.py"
        ]
      },
      "vscode": {
        "extensions": [
          "ms-python.python",
          "ms-vscode.cpptools-extension-pack",
          "xaver.clang-format"
        ]
      }
    },
    "remoteUser": "vscode"
  }